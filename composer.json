{
    "name": "youco/prj_mvc",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "yassine",
            "email": "yassnaiki@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "monolog/monolog": "^2.0",
        "vlucas/phpdotenv": "^5.6",
        "twig/twig": "^3.19",
        "ext-pdo_pgsql": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.0",
        "doctrine/migrations": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true
    }
}
